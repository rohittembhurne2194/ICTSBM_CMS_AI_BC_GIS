<input type="hidden" value="@ViewBag.myURL" id="url" />
<input type="hidden" value="@ViewBag.Username" id="user" />
<input type="hidden" value="@ViewBag.Password" id="pass" />
<input type="hidden" value="" id="demo" />

<iframe id="iframe" width="100%" height="600px" src=""></iframe>

<script type="text/javascript">

    $(document).ready(function () {

        debugger
        var url = document.getElementById("url").value;
        var user = document.getElementById("user").value;
        var pass = document.getElementById("pass").value;


        var encoded = encodeURIComponent(pass);

        
        var finalurl = url + "username=" + user +"&password=" + encoded;

        document.getElementById("iframe").src = finalurl;
        document.getElementById("demo").value = finalurl;

    });



</script>